[color]
  branch      = auto
  diff        = auto
  grep        = auto
  interactive = auto
  status      = auto
[alias]
  a     = add
  ad    = add
  br    = branch --all --color --verbose --verbose
  ci    = commit --verbose
  cl    = clone
  co    = checkout
  d     = diff
  dc    = diff --cached
  dd    = diff HEAD~
  ddd   = diff HEAD~~
  dddd  = diff HEAD~~~
  ddddd = diff HEAD~~~~
  ds    = diff --staged
  fe    = fetch
  fix   = commit --amend
  ft    = fetch
  g     = grep --break --color --heading --line-number --show-function
  l     = log --decorate --stat
  lo    = log --decorate --oneline
  lp    = log --decorate --patch-with-stat
  pl    = pull
  pu    = push
  pul   = pull
  pus   = push
  r     = remote --verbose
  sh    = show
  st    = status --branch --short
  sync  = !git fetch && git reset --hard FETCH_HEAD
  tree  = log --graph --oneline
  unstg = reset HEAD
[diff]
  algorithm = histogram
[merge]
  ff = false
[push]
  default = current
[interactive]
  singlekey = true
[user]
  name  = Taichi
  email = taichi@curipha.jp
[github]
  user = curipha

