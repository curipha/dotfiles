[color]
  branch      = auto
  diff        = auto
  grep        = auto
  interactive = auto
  status      = auto
[alias]
  a     = add
  ad    = add
  br    = branch --color --verbose --verbose
  ci    = commit
  cl    = clone
  co    = checkout
  d     = diff
  dc    = diff --cached
  dd    = diff HEAD~
  ds    = diff --staged
  fix   = commit --amend
  ft    = fetch
  g     = grep --break --color --heading --line-number --show-function
  l     = log --decorate --stat
  pu    = push -u origin master
  pul   = pull
  sh    = show
  st    = status --branch --short
[merge]
  ff    = false
[user]
  name  = Taichi
  email = taichi@curipha.jp
[github]
  user  = curipha

